        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="无证" checked="{{returnObj.sfyz == '无证'}}">无证</radio>
        </label>
      </radio-group>
    </view>
  </view>

  <!--防雷装置是否检测-->
  <view class='subView' wx:if="{{viewId == 'blzzsfjc'}}">
    <label class='titleLabel'>防雷装置是否检测</label>
    <view class='item'>
