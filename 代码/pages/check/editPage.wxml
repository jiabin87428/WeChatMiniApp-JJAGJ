<view class="baseView">
  <!--生产安全事故应急预案编制并备案-->
  <view class='subView' wx:if="{{viewId == 'yaba'}}">
    <label class='titleLabel'>是否有应急预案编制</label>
    <view class='item'>
      <radio-group id="sfyyabz" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="是" checked="{{returnObj.sfyyabz == '是'}}">是</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="否" checked="{{returnObj.sfyyabz == '否'}}">否</radio>
        </label>
      </radio-group>
    </view>

    <label class='titleLabel'>是否备案</label>
    <view class='item'>
      <radio-group id="sfba" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="是" checked="{{returnObj.sfba == '是'}}">是</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="否" checked="{{returnObj.sfba == '否'}}">否</radio>
        </label>
      </radio-group>
    </view>

    <label class='titleLabel'>备案日期</label>
    <view class='pickView'>
      <picker id="barq" mode="date" value="{{returnObj.barq}}" bindchange="bindDateChange">
        <view class="picker">
        {{returnObj.barq != null ? returnObj.barq : '请选择备案日期'}}
        </view>
      </picker>
    </view>
    <view class='line'></view>
  </view>

  <!--安全生产标准化达标及时间等级-->
  <view class='subView' wx:if="{{viewId == 'bzhdb'}}">
    <label class='titleLabel'>安全生产标准化是否达标</label>
    <view class='item'>
      <radio-group id="bzhdb" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="是" checked="{{returnObj.bzhdb == '是'}}">是</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="否" checked="{{returnObj.bzhdb == '否'}}">否</radio>
        </label>
      </radio-group>
    </view>

    <label class='titleLabel'>等级</label>
    <view id='pjdj' class='picker' bindtap='pickLevel'>
      {{returnObj.pjdj != null ? returnObj.pjdj : '请选择评级等级'}}
    </view>

    <label class='titleLabel'>评级日期</label>
    <view class='pickView'>
      <picker id="pjsq" mode="date" value="{{returnObj.pjsq}}" bindchange="bindDateChange">
        <view class="picker">
        {{returnObj.pjsq != null ? returnObj.pjsq : '请选择评级日期'}}
        </view>
      </picker>
    </view>
    <view class='line'></view>
  </view>

  <!--风险分级管控落实情况-->
  <view class='subView' wx:if="{{viewId == 'fxgkqk'}}">
    <label class='titleLabel'>风险分级管控是否落实</label>
    <view class='item'>
      <radio-group id="fxgkqk" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="是" checked="{{returnObj.fxgkqk == '是'}}">是</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="否" checked="{{returnObj.fxgkqk == '否'}}">否</radio>
        </label>
      </radio-group>
    </view>
  </view>

  <!--隐患排查制度落实情况-->
  <view class='subView' wx:if="{{viewId == 'yhpcls'}}">
    <label class='titleLabel'>隐患排查制度是否落实</label>
    <view class='item'>
      <radio-group id="yhpcls" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="是" checked="{{returnObj.yhpcls == '是'}}">是</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="否" checked="{{returnObj.yhpcls == '否'}}">否</radio>
        </label>
      </radio-group>
    </view>
  </view>
  
  <!--是否建立企业安全生产责任制-->
  <view class='subView' wx:if="{{viewId == 'anzrz'}}">
    <label class='titleLabel'>是否建立企业安全生产责任制</label>
    <view class='item'>
      <radio-group id="anzrz" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="是" checked="{{returnObj.anzrz == '是'}}">是</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="欠缺" checked="{{returnObj.anzrz == '欠缺'}}">欠缺</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="否" checked="{{returnObj.anzrz == '否'}}">否</radio>
        </label>
      </radio-group>
    </view>
  </view>

  <!--是否使用危险化学品-->
  <view class='subView' wx:if="{{viewId == 'sfsyhxp'}}">
    <label class='titleLabel'>是否使用危险化学品</label>
    <view class='item'>
      <radio-group id="sfsyhxp" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="是" checked="{{returnObj.sfsyhxp == '是'}}">是</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="否" checked="{{returnObj.sfsyhxp == '否'}}">否</radio>
        </label>
      </radio-group>
    </view>

    <label class='titleLabel'>管理状况</label>
    <view class='item'>
      <radio-group id="glzk" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="较好" checked="{{returnObj.glzk == '较好'}}">较好</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="一般" checked="{{returnObj.glzk == '一般'}}">一般</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="较差" checked="{{returnObj.glzk == '较差'}}">较差</radio>
        </label>
      </radio-group>
    </view>
  </view>

  <!--是否有危化品仓库或周转点-->
  <view class='subView' wx:if="{{viewId == 'sfyck'}}">
    <label class='titleLabel'>是否有危化品仓库或周转点</label>
    <view class='item'>
      <radio-group id="sfyck" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="是" checked="{{returnObj.sfyck == '是'}}">是</radio>
