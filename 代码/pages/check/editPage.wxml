  </view>

  <!--安全负责人持证情况-->
  <view class='subView' wx:if="{{viewId == 'fzrsfcz'}}">
    <label class='titleLabel'>安全负责人持证情况</label>
    <view class='item'>
      <radio-group id="fzrsfcz" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="有" checked="{{returnObj.fzrsfcz == '有'}}">有</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="无" checked="{{returnObj.fzrsfcz == '无'}}">无</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="过期" checked="{{returnObj.fzrsfcz == '过期'}}">过期</radio>
        </label>
      </radio-group>
    </view>
  </view>

  <!--安全管理员持证情况-->
  <view class='subView' wx:if="{{viewId == 'glysfcz'}}">
