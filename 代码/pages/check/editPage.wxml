      <radio-group id="glysfcz" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="有" checked="{{returnObj.glysfcz == '有'}}">有</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="无" checked="{{returnObj.glysfcz == '无'}}">无</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="过期" checked="{{returnObj.glysfcz == '过期'}}">过期</radio>
        </label>
      </radio-group>
    </view>
  </view>

  <!--是否有粉尘区域-->
  <view class='subView' wx:if="{{viewId == 'fcqy'}}">
    <label class='titleLabel'>是否有粉尘区域</label>
    <view class='item'>
      <radio-group id="fcqy" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="是" checked="{{returnObj.fcqy == '是'}}">是</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="否" checked="{{returnObj.fcqy == '否'}}">否</radio>
        </label>
      </radio-group>
    </view>
  </view>

  <!--特种设备检测情况-->
  <view class='subView' wx:if="{{viewId == 'tzsbjc'}}">
    <label class='titleLabel'>特种设备检测情况</label>
    <view class='item'>
      <radio-group id="tzsbjc" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="是" checked="{{returnObj.tzsbjc == '是'}}">是</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="否" checked="{{returnObj.tzsbjc == '否'}}">否</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="无" checked="{{returnObj.tzsbjc == '无'}}">无</radio>
        </label>
      </radio-group>
    </view>
  </view>

  <!--安全生产管理制度是否齐全-->
  <view class='subView' wx:if="{{viewId == 'aqzdsfqq'}}">
    <label class='titleLabel'>安全生产管理制度是否齐全</label>
    <view class='item'>
      <radio-group id="aqzdsfqq" class="radio-group" bindchange="radioChange">
        <label class="radio">
          <radio value="齐全" checked="{{returnObj.aqzdsfqq == '齐全'}}">齐全</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="一般" checked="{{returnObj.aqzdsfqq == '一般'}}">一般</radio>
        </label>
        <view class='line'></view>
        <label class="radio">
          <radio value="欠缺较多" checked="{{returnObj.aqzdsfqq == '欠缺较多'}}">欠缺较多</radio>
        </label>
      </radio-group>
    </view>
  </view>

  <!--是否有食堂-->
  <view class='subView' wx:if="{{viewId == 'sfyst'}}">
    <label class='titleLabel'>是否有食堂</label>
    <view class='item'>
      <radio-group id="sfyst" class="radio-group" bindchange="radioChange">
